@{
  Layout = "_Layout";
}

@using Template.Models;
@model List<Template.Models.PlaceHolder2>;

<h1>PlaceHolder2s</h1>

<ul>
  @foreach(PlaceHolder2 placeHolder2 in Model)
  {
    <li>@Html.ActionLink($"{placeHolder2.Description}", "Details", new { id = placeHolder2.PlaceHolder2Id } )</li>
  }
</ul>

<p>@Html.ActionLink("Add new placeHolder2", "Create")</p>
<p>@Html.ActionLink("Home", "Index", "Home")</p>